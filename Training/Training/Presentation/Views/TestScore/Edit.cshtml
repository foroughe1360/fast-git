
@model InterfaceEntity.TestScoreEntity

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/Style.css" rel="stylesheet" />
    <script src="~/Content/Date/js/jquery.min.js"></script>
    <script src="~/ControllerJS/TestScoreController.js"></script>
    <title></title>
</head>
<body>

    <!-- #section:basics/content.breadcrumbs -->
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try { ace.settings.check('breadcrumbs', 'fixed') } catch (e) { }
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="ace-icon fa fa-check"></i>
                <a href="#" id="message">اطلاعات با موفقیت ثبت گردید!</a>
            </li>
        </ul>
        <!-- /.breadcrumb -->
    </div>

    <!-- /section:basics/content.breadcrumbs -->
    <div class="page-content">
        <!-- /section:settings.box -->
        <div class="page-content-area">
            <br />

            <div class="row">
                <div class="col-xs-12">
                    <!-- PAGE CONTENT BEGINS -->
                    <form class="form-horizontal" role="form">
                        <!-- #section:elements.form -->
                        @Html.ValidationSummary(true)
                        @Html.HiddenFor(model => model.TestScoreId, new { @id = "TestScoreId" })

                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1">کد شناسایی ثبت نام  شرکت کنندگان برای دوره آموزشی : </label>
                            <div class="col-md-9">
                                @Html.TextBoxFor(model => model.CourseRegistrationId, new { @id = "CourseRegistrationId", @class = "col-xs-10 col-sm-5 col-md-10 fs", @placeholder = "کد شناسایی ثبت نام  شرکت کنندگان برای دوره آموزشی" })
                                @Html.ValidationMessageFor(model => model.CourseRegistrationId)
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1">نام شرکت کنندگان دوره آموزشی: </label>
                            <div class="col-md-9">
                                @Html.TextBoxFor(model => model.EmployemeName, new { @id = "EmployemeName", @class = "col-xs-10 col-sm-5 col-md-10 fs", @placeholder = "نام شرکت کنندگان برای دوره آموزشی" })
                                @Html.ValidationMessageFor(model => model.EmployemeName)
                            </div>
                        </div>
                            <div class="form-group">
                                <label class="col-sm-3 control-label no-padding-right" for="form-field-1">کد شناسایی انواع آزمون : </label>
                                <div class="col-md-9">
                                    @Html.TextBoxFor(model => model.VarietyOfTestId, new { @id = "VarietyOfTestId", @class = "col-xs-10 col-sm-5 col-md-10 fs", @placeholder = "کد شناسایی انواع آزمون" })
                                    @Html.ValidationMessageFor(model => model.VarietyOfTestId)
                                </div>
                            </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1">نام انواع آزمون: </label>
                            <div class="col-md-9">
                                @Html.TextBoxFor(model => model.VarietyOfTestName, new { @id = "VarietyOfTestName", @class = "col-xs-10 col-sm-5 col-md-10 fs", @placeholder = "نام انواع آزمون" })
                                @Html.ValidationMessageFor(model => model.VarietyOfTestName)
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1"> نمره: </label>
                            <div class="col-md-9">
                                @Html.TextBoxFor(model => model.Score, new { @id = "Score", @class = "col-xs-10 col-sm-5 col-md-10 fs", @placeholder = "نمره" })
                                @Html.ValidationMessageFor(model => model.Score)
                            </div>
                        </div>

                        <div class="breadcrumbs" id="breadcrumbs">
                            <script type="text/javascript">
                                try { ace.settings.check('breadcrumbs', 'fixed') } catch (e) { }
                            </script>

                            <div class="col-md-offset-3 col-md-9">
                                <button id="SaveUser" class="btn btn-success btn-white btn-round" type="button">
                                    <i class="ace-icon fa fa-floppy-o bigger-125"></i>
                                    ذخیره
                                </button>
                                &nbsp; &nbsp; &nbsp;
                                <button type="button" class="btn btn-danger btn-white btn-round" data-dismiss="modal" aria-label="Close">
                                    <i class="ace-icon fa fa-undo bigger-125"></i>

                                    بازگشت
                                </button>
                            </div>
                        </div>
                        </form>
                    </div>
                <!-- /.col -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /.page-content-area -->
    </div>
    <!-- /.page-content -->
</body>
</html>


