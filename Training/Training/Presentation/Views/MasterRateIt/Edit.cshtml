
@model InterfaceEntity.MasterRateItEntity

<!DOCTYPE html>

<html>

<head>

</head>

<body>

    <!-- #section:basics/content.breadcrumbs -->
    <div class="breadcrumbs" style="display:none" id="MessageOperation">
        <ul class="breadcrumb">
            <li>
                <i class="ace-icon fa fa-check"></i>
                <a href="#" id="messageOp"></a>
            </li>
        </ul>
        <!-- /.breadcrumb -->
    </div>

    <!-- /section:basics/content.breadcrumbs -->
    <div class="page-content">
        <!-- /section:settings.box -->
        <div class="page-content-area">
            <br />

            <div class="row">
                <div class="col-xs-12">
                    <!-- PAGE CONTENT BEGINS -->
                    <form class="form-horizontal" role="form">
                        <!-- #section:elements.form -->
                        @Html.ValidationSummary(true)
                        @Html.HiddenFor(model => model.MasterRateItId, new { @id = "MasterRateItId" })

                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1">عنوان امتیازدهی : </label>
                            <div class="col-md-9">
                                @Html.DropDownList("TypesRateItId", new MultiSelectList(ViewBag.TableInterfaceIdTypesRateItTemp, "Value", "Text"),"-- عنوان امتیازدهی را انتخاب نمائید --", new { @id = "TypesRateItId", @class = "col-xs-10 col-sm-5 col-md-10 fs" })
                                @Html.ValidationMessageFor(model => model.TypesRateItId)
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1">عنوان امتیاز : </label>
                            <div class="col-md-9">
                                @Html.TextBoxFor(model => model.Name, new { @id = "Name", @class = "col-xs-10 col-sm-5 col-md-10 fs", @placeholder = "عنوان امتیاز" })
                                @Html.ValidationMessageFor(model => model.Name)
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1">حداقل امتیاز : </label>
                            <div class="col-md-9">
                                @Html.TextBoxFor(model => model.MinRating, new { @id = "MinRating", @class = "col-xs-10 col-sm-5 col-md-10 fs", @placeholder = "حداقل امتیاز" })
                                @Html.ValidationMessageFor(model => model.MinRating)
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1">حداکثر امتیاز : </label>
                            <div class="col-md-9">
                                @Html.TextBoxFor(model => model.MaxRating, new { @id = "MaxRating", @class = "col-xs-10 col-sm-5 col-md-10 fs", @placeholder = "حداکثر امتیاز" })
                                @Html.ValidationMessageFor(model => model.MaxRating)
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1">عنوان امتیاز اصلی : </label>
                            <div class="col-md-9">
                                @Html.TextBoxFor(model => model.Parent, new { @id = "Parent", @class = "col-xs-10 col-sm-5 col-md-10 fs", @placeholder = "عنوان امتیاز اصلی" })
                                @Html.ValidationMessageFor(model => model.Parent)
                            </div>
                        </div>

                        <div class="breadcrumbs" id="breadcrumbs">
                            <div class="col-md-offset-3 col-md-9">
                                <button id="btnEdit" onclick="EditData()" class="btn btn-success btn-white btn-round" type="button">
                                    <i class="ace-icon fa fa-floppy-o bigger-125"></i>
                                    ذخیره
                                </button>
                                &nbsp; &nbsp; &nbsp;
                                <button type="button" class="btn btn-danger btn-white btn-round" data-dismiss="modal" aria-label="Close">
                                    <i class="ace-icon fa fa-undo bigger-125"></i>

                                    بازگشت
                                </button>
                                <img class="col-md-offset-1" style="display:none" id="ajaxloader" src="~/AceTemplate/assets/img/ajax-loader2.gif" />
                            </div>
                        </div>
                    </form>
                </div>
                <!-- /.col -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /.page-content-area -->
    </div>
    <!-- /.page-content -->
</body>

</html>



